/* pages/uncomf/uncomf.wxss */
@import '../bp/bp.wxss';
.history-list {
  display: flex;
  width: 60%;
  height: 90%;
  overflow: auto;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  padding-right: 15px;

}

.history-list-item {
  font-size: 36rpx;
  color: #188df0;
  flex-shrink: 1;
  line-height: 38rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}